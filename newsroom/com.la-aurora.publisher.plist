<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.la-aurora.publisher</string>
    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>__PROJECT_ROOT__/newsroom/daemon.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>__PROJECT_ROOT__/newsroom/logs/launchd.out.log</string>
    <key>StandardErrorPath</key>
    <string>__PROJECT_ROOT__/newsroom/logs/launchd.err.log</string>
  </dict>
</plist>
